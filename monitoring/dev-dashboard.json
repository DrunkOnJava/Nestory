{
  "dashboard": {
    "id": null,
    "uid": "nestory-dev-main",
    "title": "Nestory Development Metrics",
    "description": "Development dashboard with build metrics, error tracking, and performance monitoring",
    "tags": ["development", "builds", "errors", "performance"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Build Status Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Total Successful Builds",
        "type": "stat",
        "targets": [
          {
            "expr": "build_success_total",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Build Duration (Last)",
        "type": "stat",
        "targets": [
          {
            "expr": "build_duration_seconds",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "red", "value": 120}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 4,
        "title": "Build Errors",
        "type": "stat",
        "targets": [
          {
            "expr": "build_error_total",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 15}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Build Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "build_success_total / (build_success_total + build_error_total) * 100",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 7},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "max": 100,
            "min": 0,
            "unit": "percent"
          }
        }
      },
      {
        "id": 6,
        "title": "Test Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "test_duration_seconds",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 7},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            },
            "unit": "s"
          }
        }
      }
    ],
    "schemaVersion": 41,
    "version": 1
  },
  "overwrite": true
}